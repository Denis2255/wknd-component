<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[]"
            cq:template="/conf/wknd/settings/wcm/templates/xf-web-variation"
            cq:xfMasterVariation="{Boolean}true"
            cq:xfVariantType="web"
            jcr:primaryType="cq:PageContent"
            jcr:title="Footer"
            sling:resourceType="wknd/components/xfpage">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wknd/components/container"
                layout="responsiveGrid">
            <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/textimage"/>
            <separator
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/separator"/>
            <share
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/sharecomponent"
                    title="NewName">
                <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title of Component"
                        name="./title"
                        sling:hideResource="{Boolean}true"/>
                <links
                        jcr:primaryType="nt:unstructured">
                    <item0
                            jcr:primaryType="nt:unstructured"
                            firstNameLink="Cookie Policy"
                            secondNameLink="Privacy Policy"
                            thirdNameLink="support"/>
                </links>
            </share>
            <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/text"
                    text="Â© World Handicap System 2022. All Rights Reserved."/>
        </root>
    </jcr:content>
</jcr:root>
